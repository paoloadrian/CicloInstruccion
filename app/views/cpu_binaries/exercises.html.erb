<h1>Ejercicios en Binario</h1>
<div class="container">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Fecha</th>
        <th>Puntuaci√≥n</th>
      </tr>
    </thead>
    <tbody>
      <% for i in 1..@exercises.size%>
        <% exercise = @exercises[@exercises.size - i] %>
        <tr>
        <% if exercise.ram_binary_id == nil %>
          <th><a href="/ram_binaries/<%=exercise.id%>"><%= exercise.name %></a></th>
        <% else %>
          <th><a href="/binary_cycles/<%=exercise.binary_cycle_id%>"><%= exercise.name %></a></th>
        <% end %>
        <th><%= exercise.updated_at %></th>
        <th><%= exercise.get_score %></th>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
<% stylesheet params[:controller] %>