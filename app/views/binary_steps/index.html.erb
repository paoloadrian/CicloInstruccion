<h1>Listing binary_steps</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Origin</th>
      <th>Destiny</th>
      <th>Pc</th>
      <th>Actual instruction</th>
      <th>Step</th>
      <th>Execution cycle</th>
      <th>Binary cycle</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @binary_steps.each do |binary_step| %>
      <tr>
        <td><%= binary_step.content %></td>
        <td><%= binary_step.origin %></td>
        <td><%= binary_step.destiny %></td>
        <td><%= binary_step.pc %></td>
        <td><%= binary_step.actual_instruction %></td>
        <td><%= binary_step.step %></td>
        <td><%= binary_step.execution_cycle %></td>
        <td><%= binary_step.binary_cycle %></td>
        <td><%= link_to 'Show', binary_step %></td>
        <td><%= link_to 'Edit', edit_binary_step_path(binary_step) %></td>
        <td><%= link_to 'Destroy', binary_step, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Binary step', new_binary_step_path %>
